{
    "name": "Artificer Infusions",
    "description": "Artificer infusions are extraordinary processes that rapidly turn a nonmagical object into a magic item.",
    "type": "origins:multiple",
    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": "Active ability, %s to use."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "To use your Artificer Infusions ability, use your \"Active Power (Quaternary)\" key while holding the item you wish to infuse."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "The key to change the infusion you have selected is bound to your \"Active Power (Quinary)\" key."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "You cannot infuse an item multiple times. Once an item is infused, it cannot be overwritten."
        }
    ],
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 6000,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
            "bar_index": 20,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_resource",
                "comparison": "<",
                "compare_to": 6000
            }
        }
    },
    "charge": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_resource",
            "change": 1200
        }
    },
